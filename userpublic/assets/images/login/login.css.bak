.index-page .difference {
    font-size: 5rem;
    color: white;
    position: absolute;
    top: 39.7%;
    left: 50%;
    width: 900px;
    margin-left: -450px;
    text-align: center;
}

.index-page .difference:before,
.index-page .difference:after {
    background-color: #91704D;
    content: "";
    display: inline-block;
    height: 1px;
    position: relative;
    vertical-align: middle;
    width: 25%;
}

.index-page .difference:before {
    right: 0.5em;
    margin-left: -50%;
}

.index-page .difference:after {
    left: 0.5em;
    margin-right: -50%;
}

